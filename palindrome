class Main {
    public static void main(String[] args) {
        int n=232,rev=0;
        int temp=n;
        while(n>0){
            rev=rev*10+(n%10);
            n=n/10;
        }
        System.out.println("reversed number:"+rev);
        if (temp==rev){
            System.out.println("palindrome");
        }
    }
}